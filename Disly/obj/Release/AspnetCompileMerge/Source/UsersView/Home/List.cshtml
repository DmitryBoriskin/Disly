@model PageViewModel

<div class="container">
    <div class="container">
        <ul class="breadcrumb pull-left breadcrumb-list">
            <li><a href="/">ГЛАВНАЯ</a></li>
            <li><a href="/list">СПИСОК САЙТОВ</a></li>
            @*       <li><a class="active" href="#">ОБЩЕОБРАЗОВАТЕЛНЬЫЕ УЧРЕЖДЕНИЯ</a><li> *@
        </ul>
    </div>

    <h1 id="inst-list-header" class="text-left">Образовательные учреждения Чувашской Республики</h1>

    <div class="row">

        <div class="col-xs-4">
            @*@Html.Partial("/UsersView/Home/SearchPanel.cshtml")*@
        </div>

        <div class="col-xs-8">
            <div id="search-input-list">
                <div class="stylish-input-group">
                    <form method="get" action="/list">
                        <input autocomplete="off" name='keyword' id="input-search" type="text" class="form-control" placeholder="Введите полное или краткое наименование организации..." value="">
                        <ul id="autocomplete" class="list-group autocomplete-group"></ul>
                        <span class="input-group-addon submit-button-background">
                            <button type="submit" onClick="search();">
                                <img src="~/content/Front/img/search.png">
                            </button>
                        </span>
                    </form>
                </div>
            </div>
            <h1 class="header-search-result">Результаты поиска</h1>
            <div class="map-block panel panel-default" style="display: none;">
                <div class="text">
                    <i class="fontello-icon icon-resize-full"></i>РАЗВЕРНУТЬ КАРТУ
                </div>
            </div>
            @*@Html.Partial("ListView")*@
        </div>
    </div>

