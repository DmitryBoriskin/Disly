@{
    ViewBag.Version = "1.0.1";
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="keywords" content="@ViewBag.keywords">
    <meta name="description" content="@ViewBag.description">


    @* <link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css"> *@



    <link href="/content/Front/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="/content/Front/css/Style.css" rel="stylesheet" type="text/css" />
    <link href="/content/Front/css/simplelightbox.min.css" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="/content/css/fontello.css"><!--[if IE 7]><link rel="stylesheet" href="/content/css/fontello-ie7.css"><![endif]-->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">


    <title> @ViewBag.Title </title>
</head>

<body>
    <div id="@ViewBag.background_id">@*background-layer-main*@
        <header>
            @Html.Partial("menu")
        </header>

        @RenderBody()
    </div>
    @Html.Partial("footer")
    @*<div class="pege-path">
        @Html.Partial("Partial/PagePathPartial")
    </div>*@


    <script src="/content/Front/js/jquery-2.2.1.min.js" type="text/javascript"></script>
    <script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>
    <script src="/content/Front/js/simple-lightbox.js" type="text/javascript"></script>
    <script src="/content/Front/js/bootstrap.min.js" type="text/javascript"></script>
    <script src="/content/Front/js/salvattore.min.js"></script>
    <script src="/content/Front/js/main.js?version=@ViewBag.Version" type="text/javascript"></script>
    <script src="/content/Front/js/cookie.js?version=@ViewBag.Version" type="text/javascript"></script>
    <script src="/content/Front/js/button.js?version=@ViewBag.Version" type="text/javascript"></script>
    <script src="/content/Front/js/design.js?version=@ViewBag.Version" type="text/javascript"></script>

</body>


</html>