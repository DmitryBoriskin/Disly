@model PageViewModel
<div>
@if (Model.Breadcrumbs != null)
{
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="/">Главная</a></li>
        @foreach (var item in Model.Breadcrumbs)
        {
            <li class="breadcrumb-item"><a href="@item.Url">@item.Title</a></li>
        }
    </ol>
}
</div>


